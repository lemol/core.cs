{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Tests",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}/src/Domain.Test/bin/Debug/netstandard2.0/Lemolsoft.Framework.Domain.Test.dll",
            "args": [],
            "cwd": "${workspaceRoot}/src/Domain.Test",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}